<%= simple_form_for [@club, @booking], html: { data: { controller: 'flatpickr', flatpickr_dates_value: @club.flatpickr_booked_dates_object } } do |f| %>
  <%= f.input :start_date, as: :string, label: false, input_html: { class: 'd-none', data: { flatpickr_target: 'startDateInput', } } %>
  <%= f.input :end_date, as: :string, label: false, input_html: { class: 'd-none', data: { flatpickr_target: 'endDateInput' } } %>
  <%= f.submit 'Book now!', class: 'btn btn-primary' %>
<% end %>
